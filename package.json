{
    "name": "fachascript-vscode-extension",
    "displayName": "FachaScript",
    "description": "Extensi√≥n para ejecutar y editar FachaScript 0.1 en VS Code.",
    "version": "0.1.0",
    "publisher": "Pibe Emoji Studios",
    "engines": {
        "vscode": "^1.60.0"
    },
    "activationEvents": [
        "onLanguage:fchascript-indentado",
        "onLanguage:fchascript-brackets"
    ],
    "main": "./extension.js",
    "contributes": {
        "languages": [
            {
                "id": "fachascript-indentado",
                "aliases": ["FachaScript (Indentado)"],
                "extensions": [".fch", ".facha"],
                "configuration": "./language-configuration/fachascript-indentado.json"
            },
            {
                "id": "fachascript-brackets",
                "aliases": ["FachaScript (Brackets)"],
                "extensions": [".fchs"],
                "configuration": "./language-configuration/fachascript-brackets.json"
            }
        ],
        "commands": [
            {
                "command": "extension.runFachaScript",
                "title": "Ejecutar FachaScript"
            }
        ]
    },
    "dependencies": {}
}
